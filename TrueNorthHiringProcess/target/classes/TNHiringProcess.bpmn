      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_1" bpmnElement="SendForDataProcessing" sourceElement="_BPMNShape_StartEvent_3" targetElement="_BPMNShape_ServiceTask_2">
        <di:waypoint xsi:type="dc:Point" x="126.0" y="202.0"/>
        <di:waypoint xsi:type="dc:Point" x="176.0" y="202.0"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNShape id="_BPMNShape_UserTask_2" bpmnElement="JRReview">
        <dc:Bounds height="80.0" width="100.0" x="326.0" y="162.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_3" bpmnElement="SendForJRReview" sourceElement="_BPMNShape_ServiceTask_2" targetElement="_BPMNShape_UserTask_2">
        <di:waypoint xsi:type="dc:Point" x="276.0" y="202.0"/>
        <di:waypoint xsi:type="dc:Point" x="326.0" y="202.0"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNShape id="_BPMNShape_UserTask_4" bpmnElement="JRLeadReview">
        <dc:Bounds height="80.0" width="100.0" x="476.0" y="162.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_5" bpmnElement="SendForHRLeadReview" sourceElement="_BPMNShape_UserTask_2" targetElement="_BPMNShape_UserTask_4">
        <di:waypoint xsi:type="dc:Point" x="426.0" y="202.0"/>
        <di:waypoint xsi:type="dc:Point" x="476.0" y="202.0"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNShape id="_BPMNShape_ExclusiveGateway_2" bpmnElement="ApprovedGateway" isMarkerVisible="true">
        <dc:Bounds height="50.0" width="50.0" x="626.0" y="177.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_6" bpmnElement="SendForDecisionRouting" sourceElement="_BPMNShape_UserTask_4" targetElement="_BPMNShape_ExclusiveGateway_2">
        <di:waypoint xsi:type="dc:Point" x="576.0" y="202.0"/>
        <di:waypoint xsi:type="dc:Point" x="626.0" y="202.0"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_7" bpmnElement="SendForCorrection" sourceElement="_BPMNShape_ExclusiveGateway_2" targetElement="_BPMNShape_UserTask_2">
        <di:waypoint xsi:type="dc:Point" x="651.0" y="177.0"/>
        <di:waypoint xsi:type="dc:Point" x="651.0" y="106.0"/>
        <di:waypoint xsi:type="dc:Point" x="376.0" y="106.0"/>
        <di:waypoint xsi:type="dc:Point" x="376.0" y="162.0"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds height="6.0" width="6.0" x="619.0" y="106.0"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNShape id="_BPMNShape_EndEvent_2" bpmnElement="JRApproved">
        <dc:Bounds height="36.0" width="36.0" x="726.0" y="184.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_8" bpmnElement="SendForJRCompleting" sourceElement="_BPMNShape_ExclusiveGateway_2" targetElement="_BPMNShape_EndEvent_2">
        <di:waypoint xsi:type="dc:Point" x="676.0" y="202.0"/>
        <di:waypoint xsi:type="dc:Point" x="726.0" y="202.0"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
